{
  "name": "imagible-backend",
  "version": "1.0.0",
  "description": "Imagible backend",
  "author": "ArhamSoft",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js -i max",
    "dev": "nodemon ./src/index.js",
    "pm2": "pm2 start ./src/index.js --name=nftbackend",
    "pm2-restart": "pm2 restart nftbackend",
    "pm2-stop": "pm2 stop nftbackend",
    "pm2-list": "pm2 list",
    "pm2-delete": "pm2 delete nftbackend",
    "stop": "taskkill -f -im node.exe"
  },
  "keywords": [
    "IPFS",
    "Imagible",
    "NFT",
    "nodejs",
    "ArhamSoft",
    "API",
    "ArhamSoft NodeJS API"
  ],
  "dependencies": {
